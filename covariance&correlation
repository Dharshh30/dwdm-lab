A <- c(18, 28, 20)  # Preferences for Photograph A
B <- c(22, 40, 10)  # Preferences for Photograph B
C <- c(5, 7, 9)     # Age groups (5-6 years, 7-8 years, 9-10 years)

# 1. Covariance between B and C
print(cov(B, C))

# 2. Covariance matrix for A and B
print(cov(cbind(A, B)))

# 3. Correlation between B and C
print(cor(B, C))

# 4. Correlation matrix for A and B
print(cor(cbind(A, B)))
